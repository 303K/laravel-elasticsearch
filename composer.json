{
	"name": "cviebrock/laravel-elasticsearch",
	"description": "An easy way to use the official ElasticSearch client in your Laravel applications",
	"keywords": [
		"laravel",
		"elasticsearch",
		"search",
		"client"
	],
	"require": {
		"php": ">=5.4.0",
		"elasticsearch/elasticsearch": "^1.3",
		"illuminate/support": "~4|~5",
		"psr/log": "~1.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Colin Viebrock",
			"email": "colin@viebrock.ca"
		}
	],
	"autoload": {
		"psr-4": {
			"Cviebrock\\LaravelElasticsearch\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
